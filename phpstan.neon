includes:
  - %currentWorkingDirectory%/.Build/vendor/friendsoftypo3/phpstan-typo3/extension.neon # this takes care of the TYPO3 specific constants
parameters:
  level: 7
  paths:
    - %currentWorkingDirectory%/Classes

  checkMissingIterableValueType: false
  ignoreErrors:
    - '#Unsafe usage of new static\(\).#'
    - '#^Cannot call method fetchAllAssociative#'